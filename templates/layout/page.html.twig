<header class="univHead col-lg-12 col-md-12 col-sm-12 col-xs-12">
	<div class="col-lg-12 col-md-12 col-sm-12 col-xs-11">
		<div class="brand-nav navbar-header navbar-left col-lg-3 col-md-3 col-sm-11 col-xs-11">
			{{ page.univBrand }}
		</div>
		<nav class="navbar-right col-lg-9 col-md-9 col-sm-1 col-xs-1">
			<div class="mobile-menu col-sm-1 col-xs-1">
				<button onclick="revealNav()" class="glyphicon glyphicon-menu-hamburger"></button>
			</div>
			{{ page.univHead}}
		</nav>
	</div>
</header>

<div id="mobile-nav">
	{{ page.univHead }}
</div>
  
<!-- Picture background on site -->
<section class="libBackground">
</section>


<main role="main">
	<a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

	<div class="layout-content">
		<div class="row">
			<div class="NicLib-Head">
				<div class="container">
					{{ page.libHead }}
					<!--<div id="hours"> Today's Hours: <a href="/hours" title="Click for more information about hours and holidays">
						<script src="../../NicDate.js"></script>
						<script type="text/javascript">document.write(NicDate(0));</script> </a>
					</div>-->
				</div>
			</div>
			<div class="hiddenMenu">
				<div class="container">
					<i class="glyphicon glyphicon-remove" style="float:right;cursor:pointer"></i>
					<div class="aboutMenu">
						{{ page.hiddenMenu1 }}
					</div>
					<div class="servicesMenu">
						{{ page.hiddenMenu2 }}
					</div>
					<div class="collectionsMenu">
						{{ page.hiddenMenu3 }}
					</div>
					<div class="helpMenu">
						{{ page.hiddenMenu4 }}
					</div>
				</div>
			</div>
		</div>

		<!-- Main Content on page -->
		<div class="container">
			<div class="site-content">
				<div class="row">
					<div class="container">
						<div class="mainContent">
							{{ page.content }}
						</div>
					</div>
				</div>
				<div class="row footer" role="contentinfo">
					<div class="col-lg-3 col-sm-12 footer1">
						<div id="hours"> Today's Hours:<br/> <a href="/hours" title="Click for more information about hours and holidays">
							<script src="../../NicDate.js"></script>
							<script type="text/javascript">document.write(NicDate(0));</script> </a>
						</div>
						{{ page.footer }}
					</div>
					<div class="col-lg-3 footer2">
						{{ page.footer2 }}
					</div>
					<div class="col-lg-3 footer3">
						{{ page.footer3 }}
					</div>
					<div class="col-lg-3 col-sm-12 footer4">
						{{ page.footer4 }}
					</div>
				</div>
			</div>
		</div>
	</div>{# /.layout-content #}
	
</main>


{# /.layout-container #}