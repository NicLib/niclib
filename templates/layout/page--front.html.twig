  <header>
    <div class="container">
		<div class="univHead">
			{{ page.univhead }}
			<nav>
				{{ page.univlinks}}
			</nav>
		</div>
	</div>
  </header>

<div class="container">


  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">
		<div class="NicLib-Head">
			{{ page.header }}
		</div>
		<div class="mainContent">
			{{ page.content }}
		</div>
    </div>{# /.layout-content #}
	
  </main>

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# /.layout-container #}
